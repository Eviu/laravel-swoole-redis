    "keywords": [
        "laravel swoole redis pool",
        "laravel swoole redis cache",
        "laravel swoole session driver"
    ],
    "homepage": "https://github.com/antyblin/laravel-swoole-redis",
    "authors": [
        {
            "name": "Falco Lee",
            "email": "skylzl1007@gmail.com",
            "homepage": "https://github.com/falcolee",
            "role": "Developer"
        },
        {
            "name": "Pavel Kotlyarov",
            "email": "antyblin@gmail.com",
            "homepage": "https://github.com/antyblin",
            "role": "Developer"
        }
