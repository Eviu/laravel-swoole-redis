    "require": {
        "php": ">=7.4",
        "illuminate/support": "^5.0|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "swoole/ide-helper": "@dev",
        "laravel/framework": "^8.1"
    },
    "autoload": {
        "psr-4": {
            "Antyblin\\SwooleRedis\\": "src"
        }
    },
    "autoload-dev": {
